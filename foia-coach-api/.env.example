# Django Configuration
DJANGO_SETTINGS_MODULE=config.settings.local
DJANGO_SECRET_KEY=your-secret-key-here

# Database Configuration
POSTGRES_DB=muckrock
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-password-here
POSTGRES_HOST=muckrock_postgres
POSTGRES_PORT=5432

# Gemini API Configuration
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_FILE_SEARCH_STORE_NAME=StatePublicRecordsStore

# MuckRock API Configuration
# For local Docker network: http://dev.muckrock.com (requires /etc/hosts mapping)
# For production: https://muckrock.com
MUCKROCK_API_URL=http://dev.muckrock.com
MUCKROCK_API_TOKEN=your-muckrock-api-token-here
